{
  "rules": {
    "no-plusplus": 0,
    "linebreak-style": 0,
    "no-console": 0,
    "class-methods-use-this": 0
  },
  "globals": {
    "fetch": false
  },
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/standard"],
  "plugins": ["prettier"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ios.js", ".android.js"]
      }
    }
  }
}
